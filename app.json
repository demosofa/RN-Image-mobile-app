{
  "expo": {
    "name": "RN-Image-app",
    "slug": "RN-Image-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }, 
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow RN Image App to access your photos.",
          "savePhotosPermission": "Allow RN Image App to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
